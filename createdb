createdb tradesportsdb;

# to test the POST Method
curl --data "text=test&complete=false" http://127.0.0.1:3000/produto

# to test the UPDATE method
curl -X PUT --data "text=test&complete=true" http://127.0.0.1:3000/produto/1

# to test the DELETE method
curl -X DELETE http://127.0.0.1:3000/produto/1
